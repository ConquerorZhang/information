<view class="content"><radio-group class="radio-group" name="{{name}}" data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{labelList}}" wx:for-item="item" wx:for-index="index" wx:key="value"><label data-event-opts="{{[['tap',[['labelBtn',['$0',index],[[['labelList','value',item.value,'value']]]]]]]}}" class="{{[item.checked?'checkbox selectBox':'checkbox ']}}" bindtap="__e"><radio hidden="{{!(false)}}" class="radio" value="{{item.value}}" checked="{{item.checked}}"></radio>{{item.name+''}}<block wx:if="{{showsorticon}}"><image class="icon_sort" src="../../static/logo.png"></image></block></label></block></radio-group></view>