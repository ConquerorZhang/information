<view class="container"><block wx:if="{{dataList.length<1}}"><view class="empty"><image class="emptyImage" src="../../static/logo.png" mode="widthFix"></image></view></block><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navigateToDetail',['$0'],[[['dataList','',index,'id']]]]]]]}}" class="message" bindtap="__e"><view class="titleRow"><text class="title">{{item.title}}</text><text class="{{[item.state==1?'read':'unread']}}">{{item.state==1?"已读":"未读"}}</text></view><block wx:if="{{item.image.length>0}}"><image src="{{item.image}}" mode="widthFix"></image></block><view class="subTitle">{{item.subtitle}}</view><view class="time">{{item.time}}</view></view></block></view>