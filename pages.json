{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
            "path" : "pages/myInfo/myInfo",
            "style" : {
				"navigationStyle":"custom"
			}
        },
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTitleText": "uni-app"
			}
		}, {
			"path": "pages/interaction/interaction",
			"style": {
				"navigationStyle": "custom"
			}
		}
        ,{
            "path" : "pages/myInfo/myMessage",
            "style" : {
				"navigationBarTitleText": "我的消息"
			}
        }
        ,{
            "path" : "pages/interaction/publish",
            "style" : {
				"navigationBarTitleText": "发布问题",
				"app-plus": {
					"titleNView": {
						"buttons": [{
								"text": "发布",
								"color":"#EB5248",
								"fontSize":"15"
							}
						]
					}
				}
			}
        }
        ,{
            "path" : "pages/interaction/publishSucceed",
            "style" : {
				"navigationBarTitleText": "发布问题"
			}
        }
        ,{
            "path" : "pages/interaction/interactionDetail",
            "style" : {
				"navigationBarTitleText": "问题详情"
			}
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"condition" : { //模式配置，仅开发期间生效
		"current": 0, //当前激活的模式(list 的索引项)
		"list": [
			{
				"name": "", //模式名称
				"path": "pages/interaction/interaction", //启动页面，必选
				"query": "" //启动参数，在页面的onLoad函数里面得到
			}
		]
	}
}
